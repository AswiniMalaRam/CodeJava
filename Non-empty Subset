import java.util.*;
import java.io.*;
class TestClass {
    public static void main(String args[] ) throws Exception
    {
        int T;
        int N;
        Scanner sc = new Scanner(System.in);
        T = sc.nextInt();
        for(int i=0;i<T;i++)
        {      
            ArrayList<Integer> list = new ArrayList<>();     
            N = sc.nextInt();
            for(int j=0;j<N;j++)
            {
                int ele = sc.nextInt();
                list.add(ele);       
            }
             if (!list.isEmpty()) 
             {
                int mini = Collections.min(list);
                 System.out.println(mini);
            }
           
        }

    }
}

     
